{
  "name": "soul-harbor-back-end",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start ./dist/bin/server.js --name soul-harbor",
    "restart": "cross-env NODE_ENV=production pm2 restart soul-harbor",
    "stop": "pm2 stop soul-harbor",
    "delete": "pm2 delete soul-harbor",
    "show": "pm2 show soul-harbor",
    "watch": "cross-env NODE_ENV=development nodemon ./dist/bin/server.js",
    "watchDev": "cross-env NODE_ENV=development nodemon -e ts,tsx --exec npx ts-node ./bin/server.ts",
    "build-ts": "tsc",
    "copy-static": "ts-node copyStatic.ts",
    "build": "npm run build-ts && npm run copy-static"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dayjs": "^1.10.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "excel-export": "^0.5.1",
    "express": "~4.16.1",
    "express-jwt": "^6.0.0",
    "express-rate-limit": "^5.3.0",
    "file-stream-rotator": "^0.5.7",
    "formidable": "^1.2.2",
    "fs-extra": "^8.1.0",
    "helmet": "^4.6.0",
    "http-errors": "~1.6.3",
    "install": "^0.13.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multiparty": "^4.2.2",
    "mysql": "^2.18.1",
    "node-schedule": "^2.0.0",
    "nodemailer": "^6.5.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "redis": "^3.1.2",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "socket.io": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.43",
    "@types/cors": "^2.8.10",
    "@types/csurf": "^1.11.1",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/express-jwt": "^6.0.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/formidable": "^1.2.1",
    "@types/fs-extra": "^8.1.1",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/md5": "^2.3.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/multiparty": "^0.0.32",
    "@types/mysql": "^2.15.16",
    "@types/node": "^14.14.16",
    "@types/node-schedule": "^1.3.2",
    "@types/nodemailer": "^6.4.1",
